<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.platform.dao.ApiActivationGeneratorLogMapper">

    <resultMap type="com.platform.entity.ActivationGeneratorLogVo" id="activationGeneratorLogMap">
        <result property="id" column="id"/>
        <result property="activationGeneratorId" column="activation_generator_id"/>
        <result property="count" column="count"/>
        <result property="password" column="password"/>
        <result property="groupId" column="group_id"/>
        <result property="length" column="length"/>
        <result property="userId" column="user_id"/>
        <result property="addTime" column="add_time"/>
        <result property="ip" column="ip"/>
		<result property="serveInfoId" column="serve_info_id"/>
    </resultMap>

	<select id="queryObject" resultType="com.platform.entity.ActivationGeneratorLogVo">
		select
			`id`,
			`activation_generator_id`,
			`count`,
			`password`,
			`group_id`,
			`length`,
			`user_id`,
			`add_time`,
			`ip`,
		    `serve_info_id`
		from nideshop_activation_generator_log
		where id = #{id}
	</select>

	<select id="queryList" resultType="com.platform.entity.ActivationGeneratorLogVo">
		select
    		`id`,
    		`activation_generator_id`,
    		`count`,
    		`password`,
    		`group_id`,
    		`length`,
    		`user_id`,
    		`add_time`,
    		`ip`,
			`serve_info_id`
		from nideshop_activation_generator_log
		WHERE 1=1
		<if test="name != null and name.trim() != ''">
			AND name LIKE concat('%',#{name},'%')
		</if>
        <choose>
            <when test="sidx != null and sidx.trim() != ''">
                order by ${sidx} ${order}
            </when>
			<otherwise>
                order by id desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(*) from nideshop_activation_generator_log
		WHERE 1=1
        <if test="name != null and name.trim() != ''">
            AND name LIKE concat('%',#{name},'%')
        </if>
	</select>
	 
	<insert id="save" parameterType="com.platform.entity.ActivationGeneratorLogVo">
		insert into nideshop_activation_generator_log(
			`id`,
			`activation_generator_id`,
			`count`,
			`password`,
			`group_id`,
			`length`,
			`user_id`,
			`add_time`,
			`ip`,
			`serve_info_id`)
		values(
			#{id},
			#{activationGeneratorId},
			#{count},
			#{password},
			#{groupId},
			#{length},
			#{userId},
			#{addTime},
			#{ip},
			#{serveInfoId})
	</insert>
	 
	<update id="update" parameterType="com.platform.entity.ActivationGeneratorLogVo">
		update nideshop_activation_generator_log 
		<set>
			<if test="activationGeneratorId != null">`activation_generator_id` = #{activationGeneratorId}, </if>
			<if test="count != null">`count` = #{count}, </if>
			<if test="password != null">`password` = #{password}, </if>
			<if test="groupId != null">`group_id` = #{groupId}, </if>
			<if test="length != null">`length` = #{length}, </if>
			<if test="userId != null">`user_id` = #{userId}, </if>
			<if test="addTime != null">`add_time` = #{addTime}, </if>
			<if test="ip != null">`ip` = #{ip},</if>
			<if test="serveInfoId != null">`serve_info_id` = #{serveInfoId}</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="delete">
		delete from nideshop_activation_generator_log where id = #{value}
	</delete>
	
	<delete id="deleteBatch">
		delete from nideshop_activation_generator_log where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>