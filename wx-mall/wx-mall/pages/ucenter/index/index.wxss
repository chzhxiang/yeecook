}
.userinfo {
	display: flex;
	flex-direction: row; 
	/* padding: 20rpx 0;  */
	align-items: center;
	background: #6AB8B6;
  width: 750rpx; 

}
.userbutton{
  background: #6AB8B6;
  height:280rpx;
  padding-top:80rpx;
  padding-bottom:80rpx;
}

.userinfo-avatar {
	width: 116rpx;
	height: 116rpx;
	/* margin: 20rpx 0; */
	border-radius: 50%;
}

.userinfo-nickname {
  display: block;
  font-size:18px;
  /* margin-top: 38rpx; */
   /* height: 50rpx; */
