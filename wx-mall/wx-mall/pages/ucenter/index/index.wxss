  float: left; 
}
.r{
  float: right; 
}
.order_detai{
  clear: both;
  width: 100%;
}
.order_detai .li{
  float: left;
  width: 25%;
  height: 160rpx;
  text-align: center;
}

.order_detai .li .a{
  clear: both;
  display:flex;
  width:100%;
  height:100%;
  flex-direction:column;
  align-items:center;
  justify-content:center;
  background: #ffffff;

}
.order_detai .icon{
  margin: 0 auto;
  display: block;
  height: 60rpx;
  width: 60rpx;
  margin-bottom: 5rpx;
}
