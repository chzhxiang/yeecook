}
.userinfo-mobile .a{
  display: flex; 
  flex-direction: row; 
  justify-content: center;
}
.userinfo-mobile .txt{  
  height: 50rpx; 
  line-height: 40rpx;
  font-size: 30rpx;
  color:#b4282d;
  border: 1px solid #b4282d;
  border-radius: 25rpx;
  padding: 5rpx 20rpx;
  margin-top:5rpx;
}
