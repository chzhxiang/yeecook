.pie {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background: white;
  position: relative;
}  
.circle {
  position: absolute; 
  top: 50%;
  left: 50%;
  z-index: 100;
  transform: translate(-50%, -50%);
  width: 50rpx;
  height: 50rpx;
  background: white;
  border-radius: 50%;
}
.left, .right {
  width: 50rpx;
  height: 100rpx;
  float: left;
  overflow: hidden;
}
.left {
  border-radius: 100rpx 0 0 100rpx;
}
.right {
  border-radius: 0 100rpx 100rpx 0;
  position: relative;
}
.right-deg-number{
  position: absolute;
  top:66rpx;
  left:85rpx;
  z-index:1000;
  font-size:20rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,57,79,1);
  opacity: 0.5;
  text-align: right;
}
.right-two-deg-number{
  position: absolute;
  top:38rpx;
  left:85rpx;
