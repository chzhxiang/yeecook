.left-deg, .right-deg {
  width: 100%;
  height: 100%;
  background:rgba(254,185,212,1);
  /* transition: all 2s; */
}
.left-deg {
  transform-origin: right center;
}
.right-deg {
  transform-origin: left center;
}
